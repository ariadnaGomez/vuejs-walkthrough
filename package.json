{
  "name": "vuejs-walkthrough",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "test:coverage": "vue-cli-service test:unit --coverage",
    "test:unit": "vue-cli-service test:unit",
    "test:watch": "vue-cli-service test:unit --watch",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|vue|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "stylelint": "stylelint '**/*.+(css|vue)' --config '.stylelintrc' --ignore-path .gitignore",
    "fix-style": "npm run stylelint -- --fix",
    "validate": "npm-run-all --parallel test:unit build check-format lint stylelint"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@testing-library/cypress": "6.0.0",
    "@testing-library/jest-dom": "5.3.0",
    "@testing-library/vue": "5.0.2",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-e2e-cypress": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-unit-jest": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "husky": "4.2.3",
    "lint-staged": "10.1.1",
    "npm-run-all": "4.1.5",
    "prettier": "^1.19.1",
    "stylelint": "13.3.0",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.0.0",
    "vue-template-compiler": "^2.6.11"
  }
}
